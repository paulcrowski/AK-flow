{
  "name": "ak-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest watch --config vitest.config.ts",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:ci": "vitest run --config vitest.config.ts --reporter=verbose",
    "test:unit": "node scripts/test-runner.js run unit",
    "test:integration": "node scripts/test-runner.js run integration",
    "test:e2e": "node scripts/test-runner.js run e2e",
    "test:list": "node scripts/test-runner.js list",
    "test:analyze": "node scripts/test-runner.js analyze",
    "nexus": "npm run dev --prefix ak-nexus",
    "nexus:build": "npm run build --prefix ak-nexus",
    "nexus:install": "npm install --prefix ak-nexus"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@supabase/supabase-js": "^2.84.0",
    "immer": "^11.0.1",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.1.0",
    "jsdom": "^27.3.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser-brunch": "^4.1.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.14"
  }
}
